library('private-pipeline-library')
library('jenkins-shared')

mavenReleasePipeline(usePublicSettingsXmlFile: true,
    onSuccess: { build, env ->
      notifyChat(env: env, currentBuild: build, room: 'sonatype-community')
    },
    onFailure: { build, env ->
      notifyChat(env: env, currentBuild: build, room: 'sonatype-community')
    }
)
